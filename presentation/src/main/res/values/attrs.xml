<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="satellite_menu_radius" format="dimension" />
    <attr name="satellite_menu_size" format="dimension" />
    <attr name="satellite_menu_direction" format="integer">
        <enum name="right_bottom" value="0" />
        <enum name="left_bottom" value="1" />
        <enum name="left_top" value="2" />
        <enum name="right_top" value="3" />
    </attr>
    <attr name="common_item_icon" format="reference" />
    <attr name="common_item_text" format="string" />
    <attr name="common_item_bottom_line" format="boolean" />
    <declare-styleable name="SatelliteMenuView">
        <attr name="satellite_menu_radius" />
        <attr name="satellite_menu_size" />
        <attr name="satellite_menu_direction" />
    </declare-styleable>
    <declare-styleable name="CommonItem">
        <attr name="common_item_icon" />
        <attr name="common_item_text" />
        <attr name="common_item_bottom_line" />
    </declare-styleable>
    <attr name="note_minHeight" format="dimension" />
    <attr name="note_maxHeight" format="dimension" />
    <attr name="note_text_size" format="dimension" />
    <attr name="note_delete_icon_top" format="dimension" />
    <attr name="note_text_color" format="color" />
    <attr name="note_hint_text_color" format="color" />
    <attr name="note_hint_text" format="string" />
    <attr name="note_delete_icon" format="reference" />
    <declare-styleable name="NoteEditText">
        <attr name="note_maxHeight" />
        <attr name="note_minHeight" />
        <attr name="note_delete_icon" />
        <attr name="note_text_size" />
        <attr name="note_text_color" />
        <attr name="note_hint_text_color" />
        <attr name="note_hint_text" />
        <attr name="note_delete_icon_top" />
    </declare-styleable>
</resources>
